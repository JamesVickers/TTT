    <body id="top-of-page">
        <header>
            <% include ../partials/header %>
        </header>

        <main id="main-content">
            <div class="previous-next-button-div">
                <image onclick="location.href='/bochi-bochi'" 
                       id="previous-button" 
                       src="/img/hex-button-left-color.png" 
                       alt="hexagonal button linking to the previous work example page"
                       tabindex="0" class="blur-focus">
                <h2 tabindex="0" class="blur-focus">Applications</h2>
                <image onclick="location.href='/freelance'" 
                       id="next-button"
                       src="/img/hex-button-right-color.png"
