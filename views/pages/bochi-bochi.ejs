      <div class="stand-out-content-div" tabindex="1">
        <h3>&quot;Happily blogging away now, thanks!&quot;</h3>
      </div>
    </main>

    <footer id="footer">
      <% include ../partials/footer %>
    </footer>

    <% include ../partials/modal %> <%include ../partials/javascript %>
